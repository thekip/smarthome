{
  "name": "smarthome",
  "version": "0.0.1",
  "scripts": {
    "watch": "forever -w index.js --color",
    "start": "forever start index.js",
    "test": "cross-env NODE_ENV=test tape tests/**/*.spec.js | tap-spec"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "chalk": "^2.3.0",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "lodash": "^4.17.5",
    "lodash-es": "^4.17.5",
    "modbus-rtu": "^0.2.1",
    "serialport": "^6.0.5",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/compression": "0.0.35",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.1.1",
    "@types/lodash": "^4.14.102",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^8.9.1",
    "@types/serialport": "^6.0.1",
    "@types/socket.io": "^1.4.31",
    "cross-env": "^1.0.8",
    "eslint": "^4.17.0",
    "jest": "^22.2.1",
    "proxyquire": "^1.8.0",
    "sinon": "^1.17.4",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "ts-jest": "^22.0.3",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
